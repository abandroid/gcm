{
    "name": "endroid/gcm",
    "description": "Endroid Google Cloud Messaging",
    "keywords": ["endroid", "gcm", "push", "notification", "notifications", "google", "cloud", "messaging", "bundle", "symfony"],
    "homepage": "https://github.com/endroid/Gcm",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl",
            "homepage": "http://endroid.nl/"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "kriswallsmith/buzz": "~0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Gcm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\Gcm\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
